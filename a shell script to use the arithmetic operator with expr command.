# vi test6
#!/bin/bash
echo Enter two numbers
read n1 n2
add=` expr $n1 + $n2 `
echo Addition of Two numbers: $add
sub=` expr $n1 - $n2 `
echo Subtraction of Two numbers: $sub
multi=` expr $n1 \* $n2 `
echo Multiplication of Two numbers: $multi
div=` expr $n1 / $n2 `
echo Division of Two numbers: $div
# chmod 777 test5
# ./test6
